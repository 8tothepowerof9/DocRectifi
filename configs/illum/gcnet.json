{
    "data": {
        "batch_size": 4,
        "num_workers": 6
    }, 
    "train": {
        "trainer": "gc",
        "epochs": 50,
        "lr": 0.00001,
        "save": false,
        "betas": [0.9, 0.999],
        "scheduler": {
            "type": "LinearLR",
            "step_size": 10,
            "gamma": 0.5,
            "min_lr": 1e-10
        }
    }, 
    "model": {
        "type": "gcnet", 
        "gc": {
            "name": "gcnet",
            "embed_dims": [128, 160, 256],
            "drop_rate": 0.0,
            "drop_path_rate": 0.0,
            "depths": [1, 1, 1]
        }
    }, 
    "eval": {
        "save": false, 
        "vis_preds": true,
        "plot_history": true
    }
}